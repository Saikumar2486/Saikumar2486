from selenium import webdriver
#from selenium.webdriver.common.keys import keys
#from selenium.webdriver.firfox.options import options

class saikumar_python_automation:
    driver = webdriver.Firefox()

    def get_title(self,url):
        self.driver.get(url)
        return (self.driver.title)

    def browser_url(self,url):
        return (self.driver.curent_url)

s = saikumar_python_automation()
url = 'https://www.guvi.in'
print(s.get.title(url))
print(s.browser_url(url))
